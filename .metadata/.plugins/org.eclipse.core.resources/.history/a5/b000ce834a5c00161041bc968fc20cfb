public class hi {
	
	public static void main(String[] args)
	{	
		int[][] grid;
		grid = new int[10][10];
		int i;
		i = 0;
		for(int column = 0; column < 10 ; column++)
		{
				
			for(int row = 0; row < 10; row++)
			{
				grid[column][row] = 0;		
				System.out.print(grid[column][row]);
			}
			System.out.println("");
		}
		System.out.println("");
		
		for(int column = 0; column < 10; column++)
		{			
			for(int row = 0; row < 10; row++){
				grid[i][row] = 1;
				System.out.print(grid[column][row]);
			}
			System.out.println("");
			column+=2;
			//System.out.println(i);
		}
	}
}