public class MultiArray
{
	private int[][] grid;
	
	public MultiArray(){
		grid = new int[10][10];
		for(int column = 0; column < 10; column++)
		{
				
			for(int row = 0; row < 10; row++)
			{
				grid[column][row] = 1;		
			}
		}
	}
		
	private void printArray(){
		for(int column = 0; column < 10; column++)
		{
			for(int row = 0; row < 10; row++)
			{
				System.out.print(grid[column][row]);
			}
		}
	}
	
	public void patternRow(){
		
	}
	
}
